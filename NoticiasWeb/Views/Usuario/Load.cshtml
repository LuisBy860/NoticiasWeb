@model NoticiasWeb.Models.Usuarios

<div class="card">
<div class="card-header">
    Actualizar Usuario
</div>
<div class="card-body">


<form asp-action="actu" asp-controller="Usuario" >
      <div class="mb-3">      
            <input type="hidden" asp-for="UsuarioId" readonly />
      </div>
      <div class="mb-5">
            <label>Nombre</label>
            <input type="text"  asp-for="Nombre" /></>
      </div>
      <div class="mb-5">
            <label>Alias</label>
            <input type="text"  asp-for="Alias" /></>
      </div>
      <div class="mb-5">
            <label>Contraseña</label>
            <input type="text"  asp-for="Contrasena" /></>
      </div>
      <div class="mb-5">
            <label>Email</label>
            <input type="text"  asp-for="Email" /></>
      </div>
      <div class="mb-5">
            <label>PrivilegioID</label>
            <input type="text" asp-for="PrivilegioID" />
      </div>
      <div class="mb-5">
            <label>SuscripcionID</label>
            <input type="text"  asp-for="SuscripcionID" /></>
      </div>
      <button  class="btn btn-primary">Actualizar</button>
<td> @Html.ActionLink("Regresar","Index","Usuario",new{@class="btn btn-warning"} ) </td>
</form>
</div>
</div>
